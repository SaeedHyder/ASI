<?xml version="1.0" encoding="utf-8"?>
<com.otaliastudios.zoom.ZoomLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical|horizontal"
    app:alignment="center"
    app:animationDuration="280"
    app:flingEnabled="true"
    app:hasClickableChildren="true"
    app:horizontalPanEnabled="true"
    app:maxZoom="5"
    app:maxZoomType="realZoom"
    app:minZoom="1"
    app:minZoomType="realZoom"
    app:overPinchable="true"
    app:overScrollHorizontal="true"
    app:overScrollVertical="true"
    app:transformation="none"
    app:transformationGravity="center"
    app:verticalPanEnabled="true"
    app:zoomEnabled="true">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="@dimen/x360"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/x5_"
            android:layout_marginStart="@dimen/x3_"
            android:layout_marginTop="@dimen/x8_"
            android:layout_marginBottom="@dimen/x6_"
            android:background="@color/d32"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/fishBow"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x50"
                android:scaleType="fitXY"
                android:src="@drawable/d32_30" />

            <ImageView
                android:id="@+id/spinsAhoy"
                android:layout_width="@dimen/x78"
                android:layout_height="@dimen/x60"
                android:layout_marginStart="@dimen/x7_"
                android:layout_toEndOf="@+id/fishBow"
                android:scaleType="fitXY"
                android:src="@drawable/d32_31" />

            <ImageView
                android:id="@+id/robortStrom"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x70"
                android:layout_below="@+id/fishBow"
                android:layout_marginTop="@dimen/x7_"
                android:scaleType="fitXY"
                android:src="@drawable/d32_29" />

            <ImageView
                android:id="@+id/wildTest"
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x50"
                android:layout_below="@+id/spinsAhoy"
                android:layout_marginStart="@dimen/x9_"
                android:layout_marginTop="@dimen/x12_"
                android:layout_toEndOf="@+id/robortStrom"
                android:scaleType="fitXY"
                android:src="@drawable/d32_28" />

            <ImageView
                android:id="@+id/spaceBasketBall"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x60"
                android:layout_below="@+id/spinsAhoy"
                android:layout_marginStart="@dimen/x15_"
                android:layout_marginTop="@dimen/x12_"
                android:layout_toEndOf="@+id/wildTest"
                android:scaleType="fitXY"
                android:src="@drawable/d32_27" />


            <ImageView
                android:id="@+id/whacker"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x50"
                android:layout_marginStart="@dimen/x12_"
                android:layout_marginTop="@dimen/x30"
                android:layout_toEndOf="@+id/spinsAhoy"
                android:scaleType="fitXY"
                android:src="@drawable/d32_32" />

            <ImageView
                android:id="@+id/bugChuncher"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x90"
                android:layout_marginEnd="@dimen/x25"
                android:layout_marginTop="@dimen/x3_"
                android:layout_toStartOf="@+id/xdSeater"
                android:paddingTop="@dimen/x5"
                android:scaleType="fitXY"
                android:src="@drawable/d32_14" />


            <ImageView
                android:id="@+id/xdSeater"
                android:layout_width="@dimen/x110"
                android:layout_height="@dimen/x130"
                android:layout_alignParentEnd="true"
                android:scaleType="fitXY"
                android:src="@drawable/d32_02" />


            <ImageView
                android:id="@+id/vrMaze"
                android:layout_width="@dimen/x100"
                android:layout_height="@dimen/x80"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/xdSeater"
                android:layout_marginTop="@dimen/x8_"
                android:src="@drawable/d32_03" />

            <ImageView
                android:id="@+id/typhoon"
                android:layout_width="@dimen/x70"
                android:layout_height="@dimen/x80"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/vrMaze"
                android:layout_marginTop="@dimen/x10_"
                android:scaleType="fitXY"
                android:src="@drawable/d32_04" />

            <ImageView
                android:id="@+id/coconutBash"
                android:layout_width="@dimen/x90"
                android:layout_height="@dimen/x80"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/typhoon"
                android:layout_marginTop="@dimen/x10_"
                android:scaleType="fitXY"
                android:src="@drawable/d32_06" />

            <ImageView
                android:id="@+id/tCover"
                android:layout_width="@dimen/x70"
                android:layout_height="@dimen/x70"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/coconutBash"
                android:layout_marginTop="@dimen/x13_"

                android:scaleType="fitXY"
                android:src="@drawable/d32_05" />

            <ImageView
                android:id="@+id/rightTable"
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x30"
                android:layout_alignTop="@+id/vrMaze"
                android:layout_toStartOf="@+id/vrMaze"
                android:scaleType="fitXY"
                android:src="@drawable/d32_01" />

            <ImageView
                android:id="@+id/babyAirHocky"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_alignTop="@+id/rightTable"
                android:layout_toStartOf="@+id/rightTable"
                android:scaleType="fitXY"
                android:src="@drawable/d32_13" />

            <ImageView
                android:id="@+id/salom"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_alignEnd="@+id/babyAirHocky"
                android:layout_alignStart="@+id/babyAirHocky"
                android:layout_below="@+id/babyAirHocky"
                android:layout_marginTop="@dimen/x20_"
                android:src="@drawable/d32_12" />

            <ImageView
                android:id="@+id/magnoBison"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_alignEnd="@+id/salom"
                android:layout_alignStart="@+id/salom"
                android:layout_below="@+id/salom"
                android:layout_marginTop="@dimen/x15_"
                android:src="@drawable/d32_11" />

            <ImageView
                android:id="@+id/foosBall"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_alignEnd="@+id/magnoBison"
                android:layout_alignStart="@+id/magnoBison"
                android:layout_below="@+id/magnoBison"
                android:layout_marginTop="@dimen/x15_"
                android:src="@drawable/d32_10" />

            <ImageView
                android:id="@+id/airHocky"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_marginTop="@dimen/x20"
                android:layout_alignEnd="@+id/foosBall"
                android:layout_alignStart="@+id/foosBall"
                android:layout_below="@+id/foosBall"
                android:src="@drawable/d32_08" />

            <ImageView
                android:id="@+id/receptionCounter"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x60"
                android:layout_marginStart="@dimen/x8"

                android:layout_alignParentBottom="true"
                android:layout_alignStart="@+id/airHocky"
                android:src="@drawable/d32_07" />

            <ImageView
                android:id="@+id/leftTable"
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x30"
                android:layout_alignTop="@+id/airHocky"
                android:layout_marginTop="@dimen/x10_"
                android:layout_toStartOf="@+id/airHocky"
                android:src="@drawable/d34_02" />

            <ImageView
                android:id="@+id/topTable"
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x30"
                android:layout_alignTop="@+id/babyAirHocky"
                android:layout_marginTop="@dimen/x10_"
                android:layout_toStartOf="@+id/babyAirHocky"
                android:src="@drawable/d34_02" />


            <ImageView
                android:id="@+id/bottomTable"
                android:layout_width="@dimen/x30"
                android:layout_height="@dimen/x30"
                android:layout_alignBottom="@+id/typhoon"
                android:layout_alignTop="@+id/typhoon"
                android:layout_marginEnd="@dimen/x10"
                android:layout_toStartOf="@+id/typhoon"
                android:src="@drawable/d34_02" />


            <RelativeLayout
                android:id="@+id/rl_left"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/robortStrom"
                android:layout_marginStart="@dimen/x5_"
                android:layout_marginTop="@dimen/x20">

                <ImageView
                    android:id="@+id/kbCobra"
                    android:layout_width="@dimen/x50"
                    android:layout_height="@dimen/x80"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_26" />

                <ImageView
                    android:id="@+id/bigBugBlast"
                    android:layout_width="@dimen/x50"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/kbCobra"
                    android:layout_alignTop="@+id/kbCobra"
                    android:layout_marginStart="@dimen/x10_"
                    android:layout_toEndOf="@+id/kbCobra"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_25" />

                <ImageView
                    android:id="@+id/theRocket"
                    android:layout_width="@dimen/x60"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/kbCobra"
                    android:layout_alignTop="@+id/kbCobra"
                    android:layout_marginStart="@dimen/x10_"
                    android:layout_toEndOf="@+id/bigBugBlast"
                    android:src="@drawable/d32_24" />

                <ImageView
                    android:id="@+id/princessFairyTest"
                    android:layout_width="@dimen/x45"
                    android:layout_height="@dimen/x70"
                    android:layout_below="@+id/kbCobra"
                    android:layout_marginTop="@dimen/x13_"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_23" />

                <ImageView
                    android:id="@+id/billyJumper"
                    android:layout_width="@dimen/x40"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/princessFairyTest"
                    android:layout_alignTop="@+id/princessFairyTest"
                    android:layout_below="@+id/kbCobra"
                    android:layout_marginStart="@dimen/x12_"
                    android:layout_marginTop="@dimen/x2"
                    android:layout_toEndOf="@+id/princessFairyTest"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_22" />

                <ImageView
                    android:id="@+id/kidsim"
                    android:layout_width="@dimen/x50"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/billyJumper"
                    android:layout_alignTop="@+id/billyJumper"
                    android:layout_below="@+id/kbCobra"
                    android:layout_marginStart="@dimen/x12_"
                    android:layout_toEndOf="@+id/billyJumper"

                    android:scaleType="fitXY"
                    android:src="@drawable/d32_21" />

                <ImageView
                    android:id="@+id/adventureBike"
                    android:layout_width="@dimen/x40"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/kidsim"
                    android:layout_alignTop="@+id/kidsim"
                    android:layout_below="@+id/kbCobra"
                    android:layout_marginStart="@dimen/x12_"
                    android:layout_toEndOf="@+id/kidsim"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_20" />

            </RelativeLayout>


            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="@dimen/x3_"

                >


                <ImageView
                    android:id="@+id/comboDx"
                    android:layout_width="@dimen/x55"
                    android:layout_height="@dimen/x70"
                    android:scaleType="fitXY"
                    android:src="@drawable/d32_19" />

                <ImageView
                    android:id="@+id/fireFigmate"
                    android:layout_width="@dimen/x45"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/comboDx"
                    android:layout_alignTop="@+id/comboDx"
                    android:layout_marginStart="@dimen/x13_"
                    android:layout_toEndOf="@+id/comboDx"
                    android:src="@drawable/d32_18" />

                <ImageView
                    android:id="@+id/monsterCatcher"
                    android:layout_width="@dimen/x35"
                    android:layout_height="@dimen/x37"
                    android:layout_alignBottom="@+id/fireFigmate"
                    android:layout_marginStart="@dimen/x14_"
                    android:layout_toEndOf="@+id/fireFigmate"
                    android:src="@drawable/d32_17" />

                <ImageView
                    android:id="@+id/monsterEnd"
                    android:layout_width="@dimen/x35"
                    android:layout_height="@dimen/x36"
                    android:layout_alignBottom="@+id/fireFigmate"
                    android:layout_marginStart="@dimen/x15_"
                    android:layout_toEndOf="@+id/monsterCatcher"
                    android:src="@drawable/d32_16" />


                <ImageView
                    android:id="@+id/superBoxes"
                    android:layout_width="@dimen/x100"
                    android:layout_height="@dimen/x55"
                    android:layout_alignEnd="@+id/monsterEnd"
                    android:layout_alignStart="@+id/comboDx"
                    android:layout_below="@+id/comboDx"
                    android:layout_marginEnd="@dimen/x3"
                    android:layout_marginTop="@dimen/x12_"

                    android:scaleType="fitXY"
                    android:src="@drawable/d32_15" />


            </RelativeLayout>


        </RelativeLayout>
    </LinearLayout>


</com.otaliastudios.zoom.ZoomLayout>