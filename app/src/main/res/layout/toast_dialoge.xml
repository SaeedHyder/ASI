<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="@dimen/x200"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical">




    <LinearLayout
        android:id="@+id/textLayout"
        android:layout_width="@dimen/x250"
        android:layout_height="@dimen/x100"
        android:layout_centerInParent="true"
        android:background="@drawable/roundshapelayout"
        android:gravity="center"
        android:orientation="vertical">

        <com.app.asi.ui.views.AnyTextView
            android:id="@+id/txt_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/x20"
            android:layout_marginRight="@dimen/x20"
            android:layout_marginTop="@dimen/x10"
            android:alpha="0.7"
            android:gravity="center"
            android:text="@string/will_be_implemented"
            android:textColor="@color/black"
            android:textSize="@dimen/x15"
            app:typeface="@string/font_normal" />


    </LinearLayout>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="@dimen/x11"

        >

        <ImageView
            android:layout_width="@dimen/x65"
            android:layout_height="@dimen/x67"
            android:background="@drawable/white_circle"
            android:padding="@dimen/x6"
            android:src="@drawable/logo4" />

    </FrameLayout>


</RelativeLayout>