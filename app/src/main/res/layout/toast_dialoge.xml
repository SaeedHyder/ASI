<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="vertical">


    <com.app.asi.ui.views.AnyTextView
        android:id="@+id/txt_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/icon"
        android:layout_marginLeft="@dimen/x20"
        android:layout_marginRight="@dimen/x20"
        android:background="@drawable/roundshapelayout"
        android:gravity="center"
        android:paddingBottom="@dimen/x20"
        android:paddingEnd="@dimen/x20"
        android:paddingStart="@dimen/x20"
        android:paddingTop="@dimen/x40"
        android:text="@string/will_be_implemented"
        android:textColor="@color/black"
        android:textSize="@dimen/x15"
        app:typeface="@string/font_normal" />


    <ImageView
        android:id="@+id/icon"
        android:layout_width="@dimen/x65"
        android:layout_height="@dimen/x67"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="@dimen/x30_"
        android:background="@drawable/white_circle"
        android:padding="@dimen/x6"
        android:src="@drawable/logo4" />


</RelativeLayout>