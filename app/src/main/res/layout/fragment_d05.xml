<?xml version="1.0" encoding="utf-8"?>
<com.otaliastudios.zoom.ZoomLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbars="vertical|horizontal"
    app:alignment="center"
    app:animationDuration="280"
    app:flingEnabled="true"
    app:hasClickableChildren="true"
    app:horizontalPanEnabled="true"
    app:maxZoom="5"
    app:maxZoomType="realZoom"
    app:minZoom="1"
    app:minZoomType="realZoom"
    app:overPinchable="true"
    app:overScrollHorizontal="true"
    app:overScrollVertical="true"
    app:transformation="none"
    app:transformationGravity="center"
    app:verticalPanEnabled="true"
    app:zoomEnabled="true">

    <LinearLayout
        android:layout_width="@dimen/x280"
        android:layout_height="match_parent"
        android:background="@color/d32"
        android:orientation="vertical">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/x4_"
            android:orientation="horizontal">


            <ImageView
                android:id="@+id/connectForHoops"
                android:layout_width="@dimen/x150"
                android:layout_height="@dimen/x90"
                android:layout_marginTop="@dimen/x8_"

                android:src="@drawable/d05_10" />

            <ImageView
                android:id="@+id/ticketMonster"
                android:layout_width="@dimen/x100"
                android:layout_height="@dimen/x50"
                android:layout_marginTop="@dimen/x18_"
                android:layout_below="@+id/connectForHoops"
                android:src="@drawable/d05_11" />

            <ImageView
                android:id="@+id/villayCrash"
                android:layout_width="@dimen/x70"
                android:layout_height="@dimen/x100"
                android:layout_marginTop="@dimen/x14_"
                android:layout_below="@+id/ticketMonster"
                android:src="@drawable/d05_12" />

            <ImageView
                android:id="@+id/pianoKeySingle"
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x50"
                android:layout_marginTop="@dimen/x14_"
                android:layout_below="@+id/villayCrash"
                android:src="@drawable/d05_13" />

            <ImageView
                android:id="@+id/scoobyDoo"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x50"
                android:layout_marginTop="@dimen/x15_"
                android:layout_below="@+id/pianoKeySingle"
                android:src="@drawable/d05_14" />

            <ImageView
                android:id="@+id/e_calw"
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x85"
                android:layout_marginTop="@dimen/x15_"
                android:layout_below="@+id/scoobyDoo"
                android:src="@drawable/d05_15" />

            <ImageView
                android:id="@+id/ticketCircus"
                android:layout_width="@dimen/x65"
                android:layout_height="@dimen/x90"
                android:layout_marginStart="@dimen/x3_"
                android:layout_marginTop="@dimen/x15_"
                android:layout_below="@+id/e_calw"
                android:src="@drawable/d05_16" />

            <ImageView
                android:id="@+id/eClawCosmic"
                android:layout_width="@dimen/x60"
                android:layout_height="@dimen/x90"
                android:layout_marginTop="@dimen/x30_"
                android:layout_below="@+id/ticketCircus"
                android:src="@drawable/d05_17" />

            <ImageView
                android:id="@+id/eClawCosmicXL"
                android:layout_width="@dimen/x100"
                android:layout_height="@dimen/x120"
                android:layout_marginTop="@dimen/x29_"
                android:layout_marginBottom="@dimen/x10_"
                android:layout_below="@+id/eClawCosmic"
                android:src="@drawable/d05_18" />

            <ImageView
                android:id="@+id/leftTable"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x40"
                android:layout_marginStart="@dimen/x20"
                android:layout_alignTop="@+id/eClawCosmic"
                android:layout_toEndOf="@+id/eClawCosmic"
                android:src="@drawable/d05_02" />

            <ImageView
                android:id="@+id/flinStone"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x80"
                android:layout_alignParentBottom="true"
                android:layout_alignParentEnd="true"
                android:layout_marginBottom="@dimen/x20"

                android:layout_marginEnd="@dimen/x30"
                android:src="@drawable/d05_09" />

            <ImageView
                android:id="@+id/trolls"
                android:layout_width="@dimen/x80"
                android:layout_height="@dimen/x80"
                android:layout_alignParentEnd="true"
                android:layout_alignTop="@+id/villayCrash"
                android:layout_marginEnd="@dimen/x35"
                android:src="@drawable/d05_01" />

            <ImageView
                android:id="@+id/topTable"
                android:layout_width="@dimen/x40"
                android:layout_height="@dimen/x40"
                android:layout_alignStart="@+id/trolls"
                android:layout_below="@+id/trolls"

                android:layout_marginTop="@dimen/x15"
                android:src="@drawable/d05_02" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/x10"
               android:layout_alignParentEnd="true"
                android:layout_below="@+id/topTable">

                <ImageView
                    android:id="@+id/prizeCube"
                    android:layout_width="@dimen/x90"
                    android:layout_height="@dimen/x60"
                    android:layout_marginStart="@dimen/x10"
                    android:src="@drawable/d05_04" />

                <ImageView
                    android:id="@+id/eClaw600Harmoney"
                    android:layout_width="@dimen/x70"
                    android:layout_height="@dimen/x110"
                    android:layout_marginTop="@dimen/x6_"
                    android:layout_marginEnd="@dimen/x10_"
                    android:layout_below="@+id/prizeCube"
                    android:src="@drawable/d05_05" />

                <ImageView
                    android:id="@+id/prizeCube3"
                    android:layout_width="@dimen/x60"
                    android:layout_height="@dimen/x70"
                    android:layout_marginStart="@dimen/x5_"
                    android:layout_marginTop="@dimen/x5_"
                    android:layout_marginEnd="@dimen/x5_"
                    android:layout_below="@+id/prizeCube"
                    android:layout_toEndOf="@+id/eClaw600Harmoney"
                    android:src="@drawable/d05_06" />

                <ImageView
                    android:id="@+id/choclateCrane"
                    android:layout_width="@dimen/x60"
                    android:layout_height="@dimen/x65"
                    android:layout_marginEnd="@dimen/x8_"
                    android:layout_marginTop="@dimen/x3"
                  android:layout_alignBottom="@+id/ticketZone"
                    android:layout_toStartOf="@+id/ticketZone"
                    android:src="@drawable/d05_08" />

                <ImageView
                    android:id="@+id/ticketZone"
                    android:layout_width="@dimen/x70"
                    android:layout_height="@dimen/x100"
                    android:layout_marginStart="@dimen/x12_"
                    android:layout_marginTop="@dimen/x5_"
                    android:layout_marginEnd="@dimen/x9_"
                    android:layout_below="@+id/prizeCube3"
                    android:layout_toEndOf="@+id/eClaw600Harmoney"
                    android:src="@drawable/d05_07" />

            </RelativeLayout>


        </RelativeLayout>


    </LinearLayout>
</com.otaliastudios.zoom.ZoomLayout>